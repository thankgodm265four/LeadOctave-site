---
import HeroNavBar from "../components/HeroNavBar.jsx";
import Footer from "../components/Footer.jsx";
import FloatingElements from "../components/FloatingElements.jsx";
import "../styles/global.css";
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Meet Our Team | LeadOctave</title>
        <meta
            name="description"
            content="Meet the passionate team behind LeadOctave, dedicated to transforming organizational leadership through proven systems and frameworks."
        />
        <link rel="icon" type="image/png" href="/image/leadoctave-logo.png" />
        <style>
            @keyframes float {
                0%,
                100% {
                    transform: translateY(0px);
                }
                50% {
                    transform: translateY(-20px);
                }
            }
            @keyframes shimmer {
                0% {
                    background-position: -1000px 0;
                }
                100% {
                    background-position: 1000px 0;
                }
            }
            @keyframes fadeInUp {
                from {
                    opacity: 0;
                    transform: translateY(30px);
                }
                to {
                    opacity: 1;
                    transform: translateY(0);
                }
            }
            .animate-float {
                animation: float 6s ease-in-out infinite;
            }
            .animate-fadeInUp {
                animation: fadeInUp 0.8s ease-out forwards;
            }
            .shimmer {
                background: linear-gradient(
                    90deg,
                    rgba(255, 255, 255, 0) 0%,
                    rgba(255, 255, 255, 0.2) 50%,
                    rgba(255, 255, 255, 0) 100%
                );
                background-size: 1000px 100%;
                animation: shimmer 3s infinite;
            }
        </style>
    </head>
    <body class="antialiased bg-gray-50">
        <FloatingElements client:load />
        <HeroNavBar client:load />

        <!-- Background Decorations -->
        <div class="fixed inset-0 pointer-events-none overflow-hidden">
            <div
                class="absolute top-20 right-20 w-96 h-96 bg-[#C19A6B]/10 rounded-full blur-3xl animate-float"
            >
            </div>
            <div
                class="absolute bottom-40 left-20 w-80 h-80 bg-[#05101E]/5 rounded-full blur-3xl animate-float"
                style="animation-delay: 2s;"
            >
            </div>
            <div
                class="absolute top-1/2 left-1/2 w-64 h-64 bg-[#C19A6B]/5 rounded-full blur-3xl animate-float"
                style="animation-delay: 4s;"
            >
            </div>
        </div>

        <main class="pt-32 pb-20 px-6 relative z-10">
            <!-- Page Header -->
            <section
                class="max-w-6xl mx-auto text-center mb-24 animate-fadeInUp"
            >
                <div
                    class="inline-flex items-center gap-2 px-6 py-3 bg-gradient-to-r from-[#C19A6B]/20 to-[#A07855]/20 backdrop-blur-sm rounded-full mb-8 border border-[#C19A6B]/30 shadow-lg"
                >
                    <div
                        class="w-2 h-2 bg-[#C19A6B] rounded-full animate-pulse"
                    >
                    </div>
                    <span
                        class="text-sm font-bold text-[#C19A6B] uppercase tracking-wider"
                        >Meet The Experts</span
                    >
                </div>
                <h1
                    class="text-5xl md:text-6xl lg:text-7xl font-serif font-bold text-[#05101E] mb-8 leading-tight"
                >
                    The Minds Behind <br />
                    <span
                        class="text-transparent bg-clip-text bg-gradient-to-r from-[#C19A6B] via-[#D4A259] to-[#A07855] animate-gradient"
                        >LeadOctave</span
                    >
                </h1>
                <p
                    class="text-xl md:text-2xl text-gray-600 max-w-3xl mx-auto leading-relaxed"
                >
                    A dedicated team of leadership architects, transforming how
                    organizations lead and scale.
                </p>
            </section>

            <!-- Founder Section -->
            <section
                class="max-w-7xl mx-auto mb-32 animate-fadeInUp"
                style="animation-delay: 0.2s;"
            >
                <div class="text-center mb-16">
                    <div
                        class="inline-flex items-center gap-2 px-5 py-2 bg-[#05101E]/5 rounded-full mb-4"
                    >
                        <svg
                            class="w-5 h-5 text-[#C19A6B]"
                            fill="currentColor"
                            viewBox="0 0 20 20"
                        >
                            <path
                                d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"
                            ></path>
                        </svg>
                        <h2 class="text-lg font-bold text-[#05101E]">
                            Founder & Visionary
                        </h2>
                    </div>
                    <p class="text-gray-600">
                        The architect behind the Octave Model™
                    </p>
                </div>

                <div class="relative group">
                    <!-- Animated border -->
                    <div
                        class="absolute -inset-1 bg-gradient-to-r from-[#C19A6B] via-[#D4A259] to-[#A07855] rounded-3xl blur-xl opacity-30 group-hover:opacity-50 transition-all duration-500"
                    >
                    </div>

                    <div
                        class="relative bg-white rounded-3xl shadow-2xl overflow-hidden"
                    >
                        <div class="grid md:grid-cols-5 gap-0">
                            <!-- Image with overlay -->
                            <div
                                class="relative md:col-span-2 h-[400px] md:h-auto overflow-hidden"
                            >
                                <img
                                    src="/image/joshua-tolofari.png"
                                    alt="Joshua Tolofari"
                                    class="w-full h-full object-cover object-top transform group-hover:scale-110 transition-transform duration-700"
                                />
                                <div
                                    class="absolute inset-0 bg-gradient-to-t from-[#05101E] via-[#05101E]/50 to-transparent opacity-60"
                                >
                                </div>

                                <!-- Floating badge -->
                                <div
                                    class="absolute top-6 left-6 px-4 py-2 bg-white/10 backdrop-blur-md rounded-full border border-white/20"
                                >
                                    <p class="text-white text-sm font-semibold">
                                        15+ Years Experience
                                    </p>
                                </div>
                            </div>

                            <!-- Content -->
                            <div
                                class="md:col-span-3 p-10 md:p-14 flex flex-col justify-center relative overflow-hidden"
                            >
                                <!-- Decorative element -->
                                <div
                                    class="absolute top-0 right-0 w-64 h-64 bg-gradient-to-br from-[#C19A6B]/10 to-transparent rounded-full blur-3xl -mr-32 -mt-32"
                                >
                                </div>

                                <div class="relative z-10">
                                    <h3
                                        class="text-4xl md:text-5xl font-serif font-bold text-[#05101E] mb-3"
                                    >
                                        Joshua Tolofari
                                    </h3>
                                    <p
                                        class="text-2xl text-transparent bg-clip-text bg-gradient-to-r from-[#C19A6B] to-[#A07855] font-bold mb-4"
                                    >
                                        A Systems Thinker Who Builds Leaders
                                    </p>

                                    <p
                                        class="text-lg text-gray-600 mb-8 italic"
                                    >
                                        I help leaders move from chaos to
                                        cadence through structures of trust,
                                        clarity, and sustainable execution.
                                    </p>

                                    <div class="flex flex-wrap gap-4">
                                        <a
                                            href="/contact"
                                            class="inline-flex items-center gap-2 px-6 py-3 bg-gradient-to-r from-[#C19A6B] to-[#A07855] text-white font-bold rounded-xl hover:shadow-xl transition-all duration-300"
                                        >
                                            Invite Joshua
                                            <svg
                                                class="w-5 h-5"
                                                fill="none"
                                                stroke="currentColor"
                                                viewBox="0 0 24 24"
                                            >
                                                <path
                                                    stroke-linecap="round"
                                                    stroke-linejoin="round"
                                                    stroke-width="2"
                                                    d="M17 8l4 4m0 0l-4 4m4-4H3"
                                                ></path>
                                            </svg>
                                        </a>
                                        <a
                                            href="/contact"
                                            class="inline-flex items-center gap-2 px-6 py-3 border-2 border-[#05101E] text-[#05101E] font-bold rounded-xl hover:bg-[#05101E] hover:text-white transition-all duration-300"
                                        >
                                            Book Keynote
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Founder Story - 3 Block Narrative -->
                <div class="mt-20 max-w-6xl mx-auto">
                    <h3
                        class="text-4xl font-serif font-bold text-center text-[#05101E] mb-16"
                    >
                        The Story Behind the System
                    </h3>

                    <div class="grid md:grid-cols-3 gap-8">
                        <!-- Block 1: The Problem -->
                        <div class="relative group">
                            <div
                                class="absolute -inset-1 bg-gradient-to-r from-red-500/20 to-orange-500/20 rounded-2xl blur opacity-40 group-hover:opacity-60 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-8 rounded-2xl shadow-lg h-full"
                            >
                                <div
                                    class="w-12 h-12 bg-red-100 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-red-600"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-2xl font-bold text-[#05101E] mb-4"
                                >
                                    The Problem
                                </h4>
                                <p class="text-gray-700 leading-relaxed">
                                    Many leaders rely on intuition and
                                    improvisation. Teams often fail not because
                                    of talent, but because leadership systems
                                    are unclear or inconsistent.
                                </p>
                            </div>
                        </div>

                        <!-- Block 2: The Insight -->
                        <div class="relative group">
                            <div
                                class="absolute -inset-1 bg-gradient-to-r from-[#C19A6B]/30 to-[#A07855]/30 rounded-2xl blur opacity-40 group-hover:opacity-60 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-8 rounded-2xl shadow-lg h-full"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/20 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-2xl font-bold text-[#05101E] mb-4"
                                >
                                    The Insight
                                </h4>
                                <p class="text-gray-700 leading-relaxed">
                                    Joshua Tolofari recognized that structure,
                                    rhythm, and alignment—not personality or
                                    charisma—determine organizational
                                    performance.
                                </p>
                            </div>
                        </div>

                        <!-- Block 3: The System -->
                        <div class="relative group">
                            <div
                                class="absolute -inset-1 bg-gradient-to-r from-green-500/20 to-emerald-500/20 rounded-2xl blur opacity-40 group-hover:opacity-60 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-8 rounded-2xl shadow-lg h-full"
                            >
                                <div
                                    class="w-12 h-12 bg-green-100 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-green-600"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-2xl font-bold text-[#05101E] mb-4"
                                >
                                    The System
                                </h4>
                                <p class="text-gray-700 leading-relaxed">
                                    He developed the Octave Model™, an 8-pillar
                                    framework that transforms chaos into
                                    clarity, enabling leaders and teams to
                                    operate predictably and sustainably.
                                </p>
                            </div>
                        </div>
                    </div>

                    <!-- Quote -->
                    <div class="mt-12 text-center">
                        <blockquote
                            class="text-2xl md:text-3xl font-serif italic text-[#05101E] max-w-4xl mx-auto"
                        >
                            "Leadership has rhythm. And when you find that
                            rhythm, everything changes."
                        </blockquote>
                        <p class="text-[#C19A6B] font-semibold mt-4">
                            — Joshua Tolofari
                        </p>
                    </div>
                </div>

                <!-- Philosophy Section -->
                <div class="mt-24 max-w-5xl mx-auto">
                    <div class="relative group">
                        <div
                            class="absolute -inset-1 bg-gradient-to-r from-[#05101E]/10 to-[#C19A6B]/10 rounded-3xl blur-xl"
                        >
                        </div>
                        <div
                            class="relative bg-gradient-to-br from-[#05101E] to-[#0A1F3D] p-12 md:p-16 rounded-3xl text-white"
                        >
                            <h3
                                class="text-4xl md:text-5xl font-serif font-bold text-center mb-6"
                            >
                                Leadership Is Not a Gift. <br
                                    class="hidden md:block"
                                />It's a System.
                            </h3>
                            <p
                                class="text-xl text-gray-300 text-center max-w-3xl mx-auto leading-relaxed"
                            >
                                Leaders succeed only when supported by clear
                                systems, trust, and alignment. Joshua works with
                                founders, executives, and teams to design
                                leadership ecosystems that endure beyond
                                personalities or crises.
                            </p>
                        </div>
                    </div>
                </div>

                <!-- Speaking Topics / Expertise -->
                <div class="mt-24">
                    <div class="text-center mb-12">
                        <h3
                            class="text-4xl font-serif font-bold text-[#05101E] mb-4"
                        >
                            Speaking & Strategic Conversations
                        </h3>
                        <p class="text-xl text-gray-600">
                            Topics Joshua speaks on with passion and expertise
                        </p>
                    </div>

                    <div class="grid md:grid-cols-3 gap-6">
                        <!-- Topic 1 -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#C19A6B] to-[#A07855] rounded-2xl blur opacity-20 group-hover:opacity-40 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/10 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-xl font-bold text-[#05101E] mb-2"
                                >
                                    Leadership System Design
                                </h4>
                                <p class="text-gray-600">
                                    Building frameworks that create predictable
                                    excellence
                                </p>
                            </div>
                        </div>

                        <!-- Topic 2 -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#C19A6B] to-[#A07855] rounded-2xl blur opacity-20 group-hover:opacity-40 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/10 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-xl font-bold text-[#05101E] mb-2"
                                >
                                    Team Alignment & Culture
                                </h4>
                                <p class="text-gray-600">
                                    Creating cohesive teams that execute with
                                    clarity
                                </p>
                            </div>
                        </div>

                        <!-- Topic 3 -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#C19A6B] to-[#A07855] rounded-2xl blur opacity-20 group-hover:opacity-40 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/10 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M12 6.253v13m0-13C10.832 5.477 9.246 5 7.5 5S4.168 5.477 3 6.253v13C4.168 18.477 5.754 18 7.5 18s3.332.477 4.5 1.253m0-13C13.168 5.477 14.754 5 16.5 5c1.747 0 3.332.477 4.5 1.253v13C19.832 18.477 18.247 18 16.5 18c-1.746 0-3.332.477-4.5 1.253"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-xl font-bold text-[#05101E] mb-2"
                                >
                                    Leadership Training
                                </h4>
                                <p class="text-gray-600">
                                    Workshops that transform how leaders think
                                    and act
                                </p>
                            </div>
                        </div>

                        <!-- Topic 4 -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#C19A6B] to-[#A07855] rounded-2xl blur opacity-20 group-hover:opacity-40 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/10 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-xl font-bold text-[#05101E] mb-2"
                                >
                                    Organizational Rhythm
                                </h4>
                                <p class="text-gray-600">
                                    Establishing cadence that drives consistent
                                    results
                                </p>
                            </div>
                        </div>

                        <!-- Topic 5 -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#C19A6B] to-[#A07855] rounded-2xl blur opacity-20 group-hover:opacity-40 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-white p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300"
                            >
                                <div
                                    class="w-12 h-12 bg-[#C19A6B]/10 rounded-xl flex items-center justify-center mb-4"
                                >
                                    <svg
                                        class="w-6 h-6 text-[#C19A6B]"
                                        fill="none"
                                        stroke="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            stroke-linecap="round"
                                            stroke-linejoin="round"
                                            stroke-width="2"
                                            d="M13 10V3L4 14h7v7l9-11h-7z"
                                        ></path>
                                    </svg>
                                </div>
                                <h4
                                    class="text-xl font-bold text-[#05101E] mb-2"
                                >
                                    High-Performance Teams
                                </h4>
                                <p class="text-gray-600">
                                    Strategic leadership for sustained
                                    excellence
                                </p>
                            </div>
                        </div>

                        <!-- Topic 6 - CTA Card -->
                        <div class="group relative">
                            <div
                                class="absolute -inset-0.5 bg-gradient-to-r from-[#05101E] to-[#0A1F3D] rounded-2xl blur opacity-40 group-hover:opacity-60 transition duration-500"
                            >
                            </div>
                            <div
                                class="relative bg-gradient-to-br from-[#05101E] to-[#0A1F3D] p-6 rounded-2xl shadow-lg hover:shadow-xl transition-all duration-300 text-white h-full flex flex-col justify-center items-center text-center"
                            >
                                <svg
                                    class="w-12 h-12 text-[#C19A6B] mb-4"
                                    fill="none"
                                    stroke="currentColor"
                                    viewBox="0 0 24 24"
                                >
                                    <path
                                        stroke-linecap="round"
                                        stroke-linejoin="round"
                                        stroke-width="2"
                                        d="M8 12h.01M12 12h.01M16 12h.01M21 12c0 4.418-4.03 8-9 8a9.863 9.863 0 01-4.255-.949L3 20l1.395-3.72C3.512 15.042 3 13.574 3 12c0-4.418 4.03-8 9-8s9 3.582 9 8z"
                                    ></path>
                                </svg>
